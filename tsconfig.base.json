      "@mfe/system-platform/feature-welcome": [
        "libs/system-platform/feature-welcome/src/index.ts"
      ],
      "@mfe/api/share/share-core": ["libs/api/share/share-core/src/index.ts"],
      "@mfe/api/share/keycloak-app": [
        "libs/api/share/keycloak-app/src/index.ts"
      ],
      "@mfe/web/share/initialize-service": [
        "libs/web/share/initialize-service/src/index.ts"
      ],
      "@mfe/system-platform/contract/domain": [
        "libs/system-platform/contract/domain/src/index.ts"
      ],
      "@mfe/system-platform/contract/feature-list": [
        "libs/system-platform/contract/feature-list/src/index.ts"
      ],
      "@mfe/system-platform/contract/feature-detail": [
        "libs/system-platform/contract/feature-detail/src/index.ts"
      ],
      "@mfe/system-platform/dept/domain": [
        "libs/system-platform/dept/domain/src/index.ts"
